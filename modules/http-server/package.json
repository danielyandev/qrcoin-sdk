{
  "name": "@danielyandev/qr-http-server",
  "version": "1.0.0",
  "description": "Qr blockchain http server",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Ruben Danielyan",
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyandev/qr-http-server.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@danielyandev/qr-blockchain": "^1.0.0",
    "@danielyandev/qr-logger": "^1.0.0",
    "@danielyandev/qr-transaction-pool": "^1.0.0",
    "body-parser": "^1.19.0",
    "express-ipfilter": "^1.1.2",
    "express": "^4.17.1"
  }
}
